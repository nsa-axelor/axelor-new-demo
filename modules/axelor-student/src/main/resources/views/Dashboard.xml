<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <dashboard name="welcomeDashboard" title="Welcome!">
    <dashlet action="chart:chart-student" />
    <dashlet action="chart:chart-student-sql" />
  </dashboard>

  <action-view name="chart-student" title="Student Chart" model="com.axelor.app.student.db.Student">
    <view type="chart" name="chart-student" />
  </action-view>

  <action-view name="chart-student-sql" title="Student Chart" model="com.axelor.app.student.db.Student">
    <view type="chart" name="chart-student-sql" />
  </action-view>

</object-views>
