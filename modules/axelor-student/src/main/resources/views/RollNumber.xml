<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid model="com.axelor.app.student.db.RollNumber" title="Roll number" name="rollnumber-grid">
    <field name="rollnumber"/>
    <field name="student" />
  </grid>

  <form name="rollnumber-form" title="Roll number" model="com.axelor.app.student.db.RollNumber">
    <panel colSpan="12">
      <field name="rollnumber" colspan="6" />
      <field name="student" colspan="6" readonly="true"/>
    </panel>
  </form>

  </object-views>
